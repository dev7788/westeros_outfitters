<h1>Edit Your Account</h1>

<%= form_for [:admin, @user] do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br></br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br></br>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br></br>
  <%= f.label :email %>
  <%= f.text_field :email%>
  <%= f.submit "Edit User" %>

<% end %>
